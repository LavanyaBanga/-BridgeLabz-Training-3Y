public class Multiplication6to9 {
    public static void main(String[] args) {
        for (int number = 6; number <= 9; number++) {
            System.out.println("Multiplication Table of " + number);
            for (int i = 1; i <= 10; i++) {
                System.out.println(number + " * " + i + " = " + (number * i));
            }
            System.out.println();
        }
    }
}
